if(document.readyState==="loading"){document.addEventListener("DOMContentLoaded",function(){showRebrandingOverlay()})}else{showRebrandingOverlay()}function showRebrandingOverlay(){if(localStorage.getItem("rebrandingOverlayShown")!=="yes"&&isRebrandingOn()){addViewportMetaForDevices();addInterstitialStyle();var body=document.getElementsByTagName("body")[0];body.classList.add("rebranding-overlay__noscroll");body.appendChild(getRebrandingModal());trackModalShownEvent()}}function isRebrandingOn(){return document.cookie.indexOf("CWC-404-rebranding%3DCWC-404-rebranding-on")>-1}function hideRebrandingOverlay(){document.getElementsByClassName("rebranding-overlay")[0].style.display="none";document.getElementsByTagName("body")[0].classList.remove("rebranding-overlay__noscroll");var metaTag=document.getElementById("rebranding-viewport-meta");document.getElementsByTagName("head")[0].removeChild(metaTag);localStorage.setItem("rebrandingOverlayShown","yes")}function addViewportMetaForDevices(){var meta=document.createElement("meta");meta.id="rebranding-viewport-meta";meta.name="viewport";meta.content="width=device-width,initial-scale=1.0";document.getElementsByTagName("head")[0].appendChild(meta)}function addInterstitialStyle(){var style=document.createElement("style");style.innerHTML=getStyle();document.getElementsByTagName("head")[0].appendChild(style)}function handleMoreInfoLinkClick(){trackMoreInformationEvent();hideRebrandingOverlay()}function trackModalShownEvent(){if(IS24&&IS24.TEALIUM&&IS24.TEALIUM.tracking&&IS24.TEALIUM.tracking.report){IS24.TEALIUM.tracking.report({pag_pagetitle:"is24.de.rebranding.interstitial"},{reportType:"UTAG_VIEW"})}}function trackMoreInformationEvent(){if(IS24&&IS24.TEALIUM&&IS24.TEALIUM.tracking&&IS24.TEALIUM.tracking.report){IS24.TEALIUM.tracking.report({evt_ga_category:"redesign",evt_ga_action:"interstitial",evt_ga_label:"read_more"})}}function getRebrandingModal(){var rebrandingDiv=document.createElement("div");rebrandingDiv.classList.add("rebranding-overlay");rebrandingDiv.innerHTML=getHTML();return rebrandingDiv}function getHTML(){return'<div class="rebranding-overlay__content">\n'+'        <img src="//www.immobilienscout24.de/etc/designs/is24/img/immoscout24.svg" class="rebranding-overlay__logo"\n'+'             alt="Immobilien Scout24 Logo"/>\n'+'        <button class="rebranding-overlay__close-button" onclick="hideRebrandingOverlay()">\n'+'            <span class="is24-icon-closing"></span>\n'+"        </button>\n"+'        <h2 class="font-light">Das <span class="rebranding-overlay__highlight">neue</span> ImmoScout24</h2>\n'+'        <p class="rebranding-overlay__info">\n'+"            Wir wollen alles rund um Immobilien leichter machen. Und fangen gleich bei uns an - mit unserem neuen Design. Warum? Damit Sie einfacher und schneller zum Ziel kommen.\n"+'            <a class="rebranding-overlay__more-info-link" href="/redesign" target="_blank" onclick="handleMoreInfoLinkClick()">Mehr erfahren</a>\n'+"        </p>\n"+'        <button class="button button-large button-secondary" onclick="hideRebrandingOverlay()">Los geht\'s!</button>\n'+"</div>"}function getStyle(){return".rebranding-overlay {\n"+"            height: 100vh;\n"+"            width: 100%;\n"+"            display: flex;\n"+"            align-items: center;\n"+"            position: fixed;\n"+"            top: 0;\n"+"            background-color: rgba(0, 0, 0, 0.7);\n"+"            z-index: 999999; /* z-index greater than smart banner (99999) but less than that of cookie consent banner (2147483647)*/\n"+"        }\n"+"\n"+"        .rebranding-overlay__content {\n"+"            width: 600px;\n"+"            height: 560px;\n"+"            margin: 0 auto;\n"+"            padding: 56px 100px;\n"+"\n"+"            border-radius: 5px;\n"+"\n"+"            position: relative;\n"+"\n"+"            background: #ffffff;\n"+'            background-image: url("//www.static-immobilienscout24.de/statpic/is24-rebranding/6db25844cedb8d0912d55dcbc88e800e_rebranding_welcome_img@2x.png");\n'+"            background-position-y: bottom;\n"+"            background-repeat: no-repeat;\n"+"            background-size: 100%;\n"+"        }\n"+"\n"+"        .rebranding-overlay__highlight {\n"+"            color: #333;\n"+"            background-repeat: no-repeat;\n"+"            background-position: 50% 50%;\n"+"            background-image: url(data:image/png;base64,);\n"+"            background-size: cover;\n"+"        }\n"+"\n"+"        .rebranding-overlay__close-button {\n"+"            height: 40px;\n"+"            width: 40px;\n"+"            padding: 0;\n"+"            background: transparent;\n"+"            border: 0;\n"+"            position: absolute;\n"+"            top: 0;\n"+"            cursor: pointer;\n"+"            right: 0;\n"+"            font-size: 16px;\n"+"        }\n"+"\n"+"        .rebranding-overlay__logo {\n"+"            margin-bottom: 35px;\n"+"            width: 157px;\n"+"            height: 32px;\n"+"        }\n"+"\n"+"        .rebranding-overlay__info {\n"+"            font-size: 16px;\n"+"            line-height: 24px;\n"+"            margin-top: 14px;\n"+"            margin-bottom: 14px;\n"+"        }\n"+"\n"+"        .rebranding-overlay__noscroll {\n"+"            overflow: hidden;\n"+"        }\n"+"\n"+"        @media (max-width: 600px){\n"+"            .rebranding-overlay__content {\n"+"                width: 100%;\n"+"                height: 100%;\n"+"                margin: 0;\n"+"                padding: 56px 84px;\n"+"                background-image: url('//www.static-immobilienscout24.de/statpic/is24-rebranding/f594aaa2ba46226e802ce1c080500101_rebranding_welcome_img_mobile_x_heighted.png');\n"+"            }\n"+"\n"+"            .rebranding-overlay__more-info-link {\n"+"                display: block;\n"+"                margin-top: 10px;\n"+"            }\n"+"        }\n"+"\n"+"        @media (max-width: 375px) {\n"+"            .rebranding-overlay__content {\n"+"                width: 100%;\n"+"                height: 100%;\n"+"                margin: 0;\n"+"                padding: 17px 32px;\n"+"                background-image: url('//www.static-immobilienscout24.de/statpic/is24-rebranding/f594aaa2ba46226e802ce1c080500101_rebranding_welcome_img_mobile_x_heighted.png');\n"+"            }\n"+"\n"+"            .rebranding-overlay__logo {\n"+"                width: 186px;\n"+"                height: 38px;\n"+"            }\n"+"\n"+"            .rebranding-overlay__close-button {\n"+"                font-size: 14px;\n"+"                margin: 8px;\n"+"            }\n"+"        }"}
